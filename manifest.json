{
	"name": "Black Menu Dev",
	"version": "2.0.6.12",
	"manifest_version": 2,
	"description": "__MSG_extDescription__",
	"default_locale": "en",
	"permissions": [
		"storage",
		"https://*.google.com/*",
		"http://*.google.com/*"
		],
	"browser_action": {
		"default_icon": "images/icon_19.png",
		"default_popup": "popup.html"
		},
	"background": {
		"scripts":["background.js"],
		"persistent": false
		},
	 "converted_from_user_script": true,
	 "icons": {
		"128": "images/icon_128.png",
		"16": "images/icon_16.png",
		"32": "images/icon_32.png",
		"48": "images/icon_48.png"
		},
	 "web_accessible_resources": [
		"contentScripts/gplus_core.js",
		"contentScripts/gplus_page.css",
		"contentScripts/maps.css",
		"contentScripts/news.css",
		"contentScripts/gmail.css",
		"contentScripts/tasks_page.css",
		"contentScripts/translate.css",
		"contentScripts/youtube.css",
		"images/maps_traffic.png",
		"images/plus-9.png",
		"images/sprite_gadget_icons.png",
		"images/sprite-21.png",
		"images/sprite-25.png",
		"images/sprite-32.png",
		"search.html"
	],
	 "content_scripts": [ {
		"all_frames": true,
		"css": [ "contentScripts/tasks_inject.css" ],
		"js": [ "contentScripts/tasks_inject.js" ],
		"matches": [ "https://www.google.com/blackmenu/tasks/*" ],
		"run_at": "document_idle"
	}, {
		"all_frames": true,
		"css": [ "contentScripts/gplus_inject.css" ],
		"js": [ "contentScripts/gplus_inject.js" ],
		"matches": [ "https://www.google.com/blackmenu/gplus/*extension=blackMenu*" ],
		"run_at": "document_idle"
	}, {
		"all_frames": true,
		"js": [ "contentScripts/gplus_page.js" ],
		"matches": [ "https://plus.google.com/*blackMenu=inner*" ],
		"run_at": "document_start"
	}, {
		"all_frames": true,
		"js": [ "contentScripts/tasks_page.js" ],
		"exclude_globs": [ ],
		"include_globs": [ "https://mail.google.com/tasks/ig*extension=blackMenu*" ],
		"matches": [ "https://mail.google.com/tasks/ig*"],
		"run_at": "document_start"
	 }, {
		"all_frames": true,
		"js": [ "contentScripts/gmail.js" ],
		"matches": [ "https://mail.google.com/mail/mu/mp/*" ],
		"run_at": "document_start"
	 }, {
		"all_frames": true,
		"css": [ "contentScripts/calendar.css" ],
		"js": [ "contentScripts/calendar.js" ],
		"matches": [ "https://www.google.com/calendar/ig*" ],
		"run_at": "document_start"
	 }, {
		"all_frames": true,
		"css": [ "contentScripts/reader.css" ],
		"js": [ "contentScripts/reader.js" ],
		"matches": [ "https://www.google.com/reader/igoogle-module*" ],
		"run_at": "document_start"
	 }, {
		"all_frames": true,
		"css": [ "contentScripts/drive.css" ],
		"js": [ "contentScripts/drive.js" ],
